    <dependency>
      <groupId>com.diva-e.parallel-test-runner</groupId>
      <artifactId>parallel-test-runner</artifactId>
      <version>1.0.0</version>
      <scope>compile</scope>
    </dependency>

    <dependency>
      <groupId>org.springframework</groupId>
