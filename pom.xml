      <version>1.2.17</version>
    </dependency>

    <!-- Test -->
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.13.2</version>
      <scope>test</scope>
    </dependency>
    <!-- test  -->
    <!-- -->

    <!--추가->
   <!- https://mvnrepository.com/artifact/com.zaxxer/HicariCP -->
